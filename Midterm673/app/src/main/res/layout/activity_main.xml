<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="@drawable/background">

    <CheckBox
        android:id="@+id/scis673"
        android:layout_width="180dp"
        android:layout_height="54dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="364dp"
        android:text="Scissors"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/notes673"
        android:layout_width="180dp"
        android:layout_height="54dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="416dp"
        android:text="Sticky notes"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="124dp"
        android:layout_height="35dp"
        android:layout_marginStart="240dp"
        android:layout_marginTop="328dp"
        android:text="Rs.180"
        android:textColor="#545456"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/calc673"
        android:layout_width="180dp"
        android:layout_height="54dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="464dp"
        android:text="Calculator"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/book673"
        android:layout_width="180dp"
        android:layout_height="54dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="316dp"
        android:text="Books"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="121dp"
        android:layout_height="33dp"
        android:layout_marginStart="240dp"
        android:layout_marginTop="532dp"
        android:text="Rs.100"
        android:textColor="#545456"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="121dp"
        android:layout_height="33dp"
        android:layout_marginStart="240dp"
        android:layout_marginTop="432dp"
        android:text="Rs.60"
        android:textColor="#545456"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="121dp"
        android:layout_height="33dp"
        android:layout_marginStart="240dp"
        android:layout_marginTop="380dp"
        android:text="Rs.150"
        android:textColor="#545456"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="121dp"
        android:layout_height="33dp"
        android:layout_marginStart="240dp"
        android:layout_marginTop="480dp"
        android:text="Rs.1100"
        android:textColor="#545456"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/pen673"
        android:layout_width="180dp"
        android:layout_height="54dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="516dp"
        android:text="Pen/Pencil"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="327dp"
        android:layout_height="49dp"
        android:gravity="center"
        android:text="Stationary Market673"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@color/black"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.553"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.071" />

    <EditText
        android:id="@+id/name673"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="132dp"
        android:ems="10"
        android:hint="Enter your Name"
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/contact673"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="200dp"
        android:ems="10"
        android:hint="Phone number"
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn673"
        android:layout_width="143dp"
        android:layout_height="49dp"
        android:layout_marginStart="124dp"
        android:layout_marginTop="628dp"
        android:fontFamily="sans-serif-black"
        android:text="ORDER"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#FF6482"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="82dp"
        android:layout_height="37dp"
        android:layout_marginStart="240dp"
        android:layout_marginTop="276dp"
        android:text="Price"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="82dp"
        android:layout_height="37dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="276dp"
        android:text="Items"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>